{
  "name": "Dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "npm-run-all clean:* ",
    "clean:css": "rimraf css",
    "clean:dist": "rimraf dist/{css/*,js/*,images/*}",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:images": "imagemin src/images/* -o dist/images",
    
    "build-dev:sass": "node-sass -style expanded --source-map false -o css sass",
    "watch:sassprefixer": "onchange sass/style.scss -- npm-run-all watch:*",
    "watch:sass": "node-sass --output-style expanded --source-map false -o css sass"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "globstar": "^1.0.0",
    "imagemin-cli": "^4.0.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {}
}
